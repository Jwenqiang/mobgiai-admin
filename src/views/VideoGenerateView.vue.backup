const handleGenerate = async () => {
  if (!prompt.value.trim()) {
    ElMessage.warning('è¯·è¾“å…¥è§†é¢‘æè¿°')
    return
  }

  generating.value = true
  generateProgress.value = 0

  // æ¨¡æ‹Ÿç”Ÿæˆè¿‡ç¨‹
  const steps = [
    { progress: 20, text: 'æ­£åœ¨åˆ†ææ‚¨çš„æè¿°...' },
    { progress: 40, text: 'æ­£åœ¨ç”Ÿæˆå¸§åºåˆ—...' },
    { progress: 60, text: 'æ­£åœ¨æ¸²æŸ“è§†é¢‘...' },
    { progress: 80, text: 'æ­£åœ¨ä¼˜åŒ–ç”»è´¨...' },
    { progress: 100, text: 'ç”Ÿæˆå®Œæˆï¼' }
  ]

  for (const step of steps) {
    await new Promise(resolve => setTimeout(resolve, 2000))
    generateProgress.value = step.progress
    progressText.value = step.text
  }

  // ç”Ÿæˆå®Œæˆï¼Œæ·»åŠ åˆ°å†å²è®°å½•
  const newVideo: VideoResult = {
    id: Date.now().toString(),
    prompt: prompt.value,
    url: 'https://sample-videos.com/zip/10/mp4/SampleVideo_1280x720_1mb.mp4',
    thumbnail: 'https://picsum.photos/400/300?random=' + Date.now(),
    model: videoModels.value.find(m => m.value === selectedModel.value)?.label || 'Seedance 1.5 Pro',
    quality: videoQualities.value.find(q => q.value === selectedQuality.value)?.label || '720P',
    duration: selectedDuration.value,
    createdAt: Date.now()
  }

  videoHistory.value.unshift(newVideo)
  generating.value = false
  
  // æ¸…ç©ºè¾“å…¥æ¡†å’Œä¸Šä¼ çš„å›¾ç‰‡
  prompt.value = ''
  firstFrameImage.value = ''
  lastFrameImage.value = ''
  
  ElMessage.success('è§†é¢‘ç”ŸæˆæˆåŠŸï¼')
}

