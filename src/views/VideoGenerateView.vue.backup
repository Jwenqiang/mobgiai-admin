const regenerateVideo = async (video: VideoResult) => {
  const result = await ElMessageBox.confirm(
    'ç¡®å®šè¦é‡æ–°ç”Ÿæˆè¿™ä¸ªè§†é¢‘å—ï¼Ÿ',
